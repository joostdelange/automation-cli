{
  "name": "@joostdelange/cli",
  "version": "1.0.4",
  "repository": "github:joostdelange/cli",
  "type": "module",
  "bin": {
    "joost": "dist/index.js",
    "j": "dist/index.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "build": "esbuild --bundle src/index.ts --platform=node --format=esm --minify --packages=external --outfile=dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint .",
    "typecheck": "tsc --project ."
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint"
  },
  "dependencies": {
    "@aws-sdk/client-account": "3.427.0",
    "@aws-sdk/client-acm": "3.427.0",
    "@aws-sdk/client-ec2": "3.427.0",
    "@aws-sdk/client-iam": "3.427.0",
    "@aws-sdk/client-kms": "3.427.0",
    "@aws-sdk/client-organizations": "3.427.0",
    "@aws-sdk/client-secrets-manager": "3.427.0",
    "@aws-sdk/client-sts": "3.427.0",
    "@aws-sdk/credential-providers": "3.427.0",
    "@inquirer/prompts": "3.2.0",
    "chalk": "5.3.0",
    "commander": "11.0.0",
    "ora": "7.0.1",
    "pg": "8.11.3",
    "signal-exit": "4.1.0"
  },
  "devDependencies": {
    "@smithy/types": "2.3.5",
    "@types/node": "20.8.4",
    "@types/pg": "8.10.5",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "esbuild": "0.19.4",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.3",
    "simple-git-hooks": "2.9.0",
    "typescript": "5.2.2"
  }
}
